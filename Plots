    #ENGLISH & ASL
        #Create subset
        GiveN_EarlyExposed <- subset(MC_GiveN, MC_GiveN$Group_4cat == "English Early" | MC_GiveN$Group_4cat == "ASL Early")
        
        #Create variables 
        Age_EarlyBoth <- GiveN_EarlyExposed$Age
        GN_EarlyBoth <- GiveN_EarlyExposed$GiveN_ALL_ceiling_conservative
        Modality_EarlyBoth <- GiveN_EarlyExposed$Group_4cat
        
        #Create plot
        Early_Exposed <- ggplot(GiveN_EarlyExposed, aes(x=Age_EarlyBoth, y=GN_EarlyBoth)) + geom_point(aes(shape=Modality_EarlyBoth, color=Modality_EarlyBoth))
        Early_Exposed + labs(x = "Age (years)", y = "Give-N Highest Quantity Correct")
    
 
#TIMING
    #EARLY ONLY
        #Create subset
        GiveN_EarlyExposed <- subset(MC_GiveN, MC_GiveN$Group_4cat == "English Early" | MC_GiveN$Group_4cat == "ASL Early")
        
        #Create variables
        Age_Early <- GiveN_EarlyExposed$Age
        GN_Early <- GiveN_EarlyExposed$GiveN_ALL_ceiling_conservative
        Timing_Early <- GiveN_EarlyExposed$Language_Timing
        
        #Create plot
        EarlyTiming <- ggplot(GiveN_EarlyExposed, aes(x=Age_Early, y=GN_Early)) + geom_point(aes(shape=Timing_Early , color=Timing_Early))
        EarlyTiming + labs(x = "Age (years)", y = "Give-N Highest Quantity Correct")
  
    #EARLY AND LATER
        #Create subset 
        TimingGroup <- subset(MC_GiveN, MC_GiveN$Group_2cat == "Early" | MC_GiveN$Group_2cat == "Later") 
        
        #Create variables
        Age_TimeBoth <- TimingGroup$Age
        GN_TimeBoth <-  TimingGroup$GiveN_ALL_ceiling_conservative
        TimeGrp <- TimingGroup$Language_Timing #CHECK THIS. DO LIKE LAST ONE... NOT THINK HAVE TO CREATE NEW VARIABLE!
    
        #Create plot 
        BothTiming <- ggplot(TimingGroup, aes(x=Age_TimeBoth, y=GN_TimeBoth)) + geom_point(aes(shape=TimeGrp, color=TimeGrp))
        BothTiming + labs(x = "Age (years)", y = "Give-N Highest Quantity Correct")

    
    
    
