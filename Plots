    #ENGLISH ONLY
        #Create variables
        Age_EE <- EE$Age
        GN_EE <- EE$GiveN_ALL_ceiling_conservative
        Modality_EE <- EE$Language_Modality
        
        #Create plot
        EnglishEarly <- ggplot(EE, aes(x=Age_EE, y=GN_EE)) + geom_point(aes(shape=Modality_EE, color=Modality_EE)) #plot will not show up at this point, but will when call it in subsequent lines or when call EnglishEarly
        EnglishEarly + labs(x = "Age (years)", y = "Give-N Highest Quantity Correct") #add x and y labels 
